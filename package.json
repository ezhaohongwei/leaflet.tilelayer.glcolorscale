{
  "name": "leaflet.tilelayer.glcolorscale",
  "version": "1.0.0",
  "description": "Custom Leaflet TileLayer using WebGL to colorize pixels according to a specified color scale",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "es/",
    "src/"
  ],
  "scripts": {
    "build": "npm run clean && npm run copy_types && rollup -c",
    "clean": "rimraf dist && mkdir -p dist/types",
    "copy_types": "cp src/types.d.ts dist/types/",
    "test": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihmeuw/leaflet.tilelayer.glcolorscale.git"
  },
  "author": "David Schneider <davschne@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ihmeuw/leaflet.tilelayer.glcolorscale/issues"
  },
  "homepage": "https://github.com/ihmeuw/leaflet.tilelayer.glcolorscale#readme",
  "keywords": [
    "leaflet",
    "raster",
    "plugin",
    "mapping",
    "webgl",
    "colorization",
    "animation"
  ],
  "devDependencies": {
    "@types/leaflet": "1.2.1",
    "@types/lodash": "4.14.85",
    "@types/lodash-es": "4.17.0",
    "@types/pngjs": "~3.3.2",
    "autoprefixer": "^9.4.2",
    "rimraf": "~2.6.2",
    "rollup": "~0.67.4",
    "rollup-plugin-glslify": "~1.0.7",
    "rollup-plugin-postcss": "~1.6.3",
    "rollup-plugin-sourcemaps": "~0.4.2",
    "rollup-plugin-typescript2": "~0.18.0",
    "tslint": "~5.0",
    "typescript": "~2.9"
  },
  "dependencies": {
    "lodash-es": "~4.17",
    "native-promise-only": "~0.8.1",
    "pngjs": "~3.3.3",
    "regl": "~1.3.9"
  },
  "peerDependencies": {
    "leaflet": "~1.2"
  }
}
